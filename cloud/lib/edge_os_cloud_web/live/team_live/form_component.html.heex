<div>
  <h2><%= @title %></h2>

  <.form
    let={f}
    for={@changeset}
    id="team-form"
    phx-target={@myself}
    phx-change="validate"
    phx-submit="save">
  
    <%= label f, :name %>
    <%= text_input f, :name %>
    <%= error_tag f, :name %>
  
    <%= label f, :admins %>
    <%= multiple_select f, :admins, ["1": 1, "2": 2] %>
    <%= error_tag f, :admins %>
  
    <div>
      <%= submit "Save", phx_disable_with: "Saving..." %>
    </div>
  </.form>
</div>
